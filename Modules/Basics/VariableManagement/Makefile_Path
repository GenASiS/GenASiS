PATH_SPECIFIERS       = $(PATH_VARIABLE_MANAGEMENT)/Specifiers
PATH_ARRAY_OPERATIONS = $(PATH_VARIABLE_MANAGEMENT)/ArrayOperations
PATH_ARRAY_ARRAYS     = $(PATH_VARIABLE_MANAGEMENT)/ArrayArrays
PATH_VARIABLE_GROUPS  = $(PATH_VARIABLE_MANAGEMENT)/VariableGroups

VPATH_VARIABLE_MANAGEMENT = \
  $(PATH_SPECIFIERS):\
  $(PATH_ARRAY_OPERATIONS):\
  $(PATH_ARRAY_ARRAYS):\
  $(PATH_VARIABLE_GROUPS):\
  $(PATH_VARIABLE_MANAGEMENT)
