PATH_GENASIS = ../../../../../../..

include $(PATH_GENASIS)/Build/Makefile_Build
VPATH := ../../../Fields:../../Increments:$(VPATH)
include ../../../Fields/Makefile_ObjectFiles
include ../../Increments/Makefile_ObjectFiles

all: Step_RK2_C__Form_Test

Step_RK2_C__Form_Test: Basics.a Manifolds.a Operations.a Fields.a \
  ProtoFields.a Increments.a ProtoIncrements.a Step_RK2_C__Form_Test.o
	$(LINK) $(FLAGS) \
          -o Step_RK2_C__Form_Test_$(MACHINE) \
          Step_RK2_C__Form_Test.o DensityWaveStep_Form.o Step_RK2_C__Form.o \
          Step_RK_C__Template.o Step_RK__Template.o \
          ProtoIncrements.a Increments.a ProtoFields.a Fields.a Operations.a \
          Manifolds.a Basics.a $(LIBRARIES)

clean:
	rm -f *.o *.a *.mod *.ld

clobber: clean
	rm -f Step_RK2_C__Form_Test_*

include $(PATH_BASICS)/Makefile_Dependencies
include $(PATH_MANIFOLDS)/Makefile_Dependencies
include $(PATH_OPERATIONS)/Makefile_Dependencies
include $(PATH_FIELDS)/Makefile_Dependencies
include $(PATH_INCREMENTS)/Makefile_Dependencies
include $(PATH_STEPS)/Makefile_Dependencies
include ../../../Fields/Makefile_Dependencies
include ../../Increments/Makefile_Dependencies

