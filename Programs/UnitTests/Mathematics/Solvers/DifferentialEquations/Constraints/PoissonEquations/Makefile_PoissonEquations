PATH_GENASIS = ../../../../../../../..

include $(PATH_GENASIS)/Build/Makefile_Build

all: LaplacianMultipoleOld_ASC__Form_Test Poisson_ASC__Form_Test 

LaplacianMultipoleOld_ASC__Form_Test: Basics.a Manifolds.a \
  LaplacianMultipoleOld_ASC__Form_Test.o
	$(LINK) $(FLAGS) \
          -o LaplacianMultipoleOld_ASC__Form_Test_$(MACHINE) \
          LaplacianMultipoleOld_ASC__Form_Test.o \
          LaplacianMultipoleOld_ASC__Kernel.o \
          LaplacianMultipoleOld_ASC__Form.o \
          LaplacianMultipoleOld_Kernel.o LaplacianMultipoleOld_Template.o \
          SetHomogeneousSphere_Command.o \
          Manifolds.a Basics.a $(LIBRARIES)

Poisson_ASC__Form_Test: Basics.a Manifolds.a Operations.a \
  Poisson_ASC__Form_Test.o
	$(LINK) $(FLAGS) \
          -o Poisson_ASC__Form_Test_$(MACHINE) \
          Poisson_ASC__Form_Test.o Poisson_ASC__Form.o Poisson_Template.o \
          LaplacianMultipoleOld_ASC__Form.o LaplacianMultipoleOld_Template.o \
          SetHomogeneousSphere_Command.o \
	  Operations.a Manifolds.a \
          Basics.a $(LIBRARIES)

clean:
	rm -f *.o *.a *.mod *.smod *.ld

clobber: clean
	rm -f LaplacianMultipoleOld_ASC__Form_Test_*
	rm -f Poisson_ASC__Form_Test_*

include $(PATH_BASICS)/Makefile_Dependencies
include $(PATH_MANIFOLDS)/Makefile_Dependencies
include $(PATH_OPERATIONS)/Makefile_Dependencies
include $(PATH_POISSON_EQUATIONS)/Makefile_Dependencies

SetHomogeneousSphere_Command.o: \
  Basics.o \
  Manifolds.o \
  SetHomogeneousSphere_Command.f90

LaplacianMultipoleOld_ASC__Form_Test.o: \
  Basics.o \
  Manifolds.o \
  LaplacianMultipoleOld_Template.o \
  LaplacianMultipoleOld_Kernel.o \
  LaplacianMultipoleOld_ASC__Form.o \
  LaplacianMultipoleOld_ASC__Kernel.o \
  SetHomogeneousSphere_Command.o \
  LaplacianMultipoleOld_ASC__Form_Test.f90

Poisson_ASC__Form_Test.o: \
  Basics.o \
  Manifolds.o \
  Operations.o \
  Poisson_ASC__Form.o \
  SetHomogeneousSphere_Command.o \
  Poisson_ASC__Form_Test.f90
