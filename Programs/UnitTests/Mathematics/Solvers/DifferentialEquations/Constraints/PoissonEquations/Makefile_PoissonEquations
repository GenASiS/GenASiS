  Basics.o \
  Manifolds.o \
  SetHomogeneousSphere_Command.o \
  LaplacianMultipole_ASC__Form.o \
  LaplacianMultipole_ASC__Form_Test.f90

Poisson_ASC__Form_Test.o: \
  Basics.o \
