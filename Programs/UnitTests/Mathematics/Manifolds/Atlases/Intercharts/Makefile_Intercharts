PATH_GENASIS = ../../../../../../..

include $(PATH_GENASIS)/Build/Makefile_Build

all: Atlas_SC__Form_Test Atlas_SC_SC__Form_Test Atlas_SC_CE__Form_Test \
     Atlas_SC_CC__Form_Test

