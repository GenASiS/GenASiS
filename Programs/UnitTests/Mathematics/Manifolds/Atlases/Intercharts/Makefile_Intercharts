clean:
	rm -f *.o *.a *.mod *.ld

clobber: clean
	rm -f Atlas_SC__Form_Test_*
	rm -f Atlas_SC_SC__Form_Test_*
	rm -f Atlas_SC_CE__Form_Test_*
