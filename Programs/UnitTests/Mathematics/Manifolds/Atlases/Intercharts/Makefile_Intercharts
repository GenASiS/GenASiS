          -o Atlas_SC__Form_Test_$(MACHINE) \
          Atlas_SC__Form_Test.o Atlas_SC__Form.o GeometryFlat_ASC__Form.o \
          Atlas_SC__Template.o Field_ASC__Template.o \
          Charts.a AtlasBasics.a Basics.a $(LIBRARIES)

