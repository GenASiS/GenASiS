PATH_GENASIS = ../../../../../..

include $(PATH_GENASIS)/Build/Makefile_Build

all: Bundle_SLL_ASC_CSLD__Form_Test

Bundle_SLL_ASC_CSLD__Form_Test: Bundle_SLL_ASC_CSLD__Form_Test.o
	$(LINK) $(FLAGS) \
          -o Bundle_SLL_ASC_CSLD__Form_Test_$(MACHINE) \
          $(BASICS) $(ATLASES) BundleHeader_Form.o FieldBundle_Template.o \
          FibersWritten_CSL__Form.o Field_BSLL_ASC_CSLD__Template.o \
