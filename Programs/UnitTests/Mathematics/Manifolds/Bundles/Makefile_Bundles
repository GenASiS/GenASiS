	rm -f *.o *.mod *.ld

clobber: clean
	rm -f Bundle_SLL_ASC_CSLD__Form_Test_*

include $(PATH_BASICS)/Makefile_Dependencies
include $(PATH_ATLASES)/Makefile_Dependencies
include $(PATH_BUNDLES)/Makefile_Dependencies

