PATH_GENASIS = ../../../../../..

include $(PATH_GENASIS)/Build/Makefile_Build

all: Fluid_D__Form_Test

Fluid_D__Form_Test: Basics.a Mathematics.a Fluid_D__Form_Test.o
	$(LINK) $(FLAGS) \
          -o Fluid_D__Form_Test_$(MACHINE) \
          Fluid_D__Form_Test.o Fluid_D__Form.o \
          Mathematics.a Basics.a $(LIBRARIES)

clean:
	rm -f *.o *.a *.mod *.ld

clobber: clean
	rm -f Fluid_D__Form_Test_*

include $(PATH_BASICS)/Makefile_Dependencies
include $(PATH_MATHEMATICS)/Makefile_Dependencies
include $(PATH_SPACES)/Makefile_Dependencies
include $(PATH_FLUIDS)/Makefile_Dependencies

Fluid_D__Form_Test.o: \
  Basics.o \
  Fluid_D__Form.o \
  Fluid_D__Form_Test.f90
