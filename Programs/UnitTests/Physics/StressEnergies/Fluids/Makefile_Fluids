PATH_GENASIS = ../../../../../..

include $(PATH_GENASIS)/Build/Makefile_Build

all: Fluid_D__Form_Test Fluid_P_I__Form_Test FluidFeatures_P__Form_Test \
     Tally_F_D__Form_Test Tally_F_P__Form_Test Sources_F__Form_Test

Fluid_D__Form_Test: Basics.a Mathematics.a Fluid_D__Form_Test.o
	$(LINK) $(FLAGS) \
          -o Fluid_D__Form_Test_$(MACHINE) \
          Fluid_D__Form_Test.o Fluid_D__Form.o FluidFeatures_Template.o \
          Mathematics.a Basics.a $(LIBRARIES)

Fluid_P_I__Form_Test: Basics.a Mathematics.a Fluid_P_I__Form_Test.o
	$(LINK) $(FLAGS) \
          -o Fluid_P_I__Form_Test_$(MACHINE) \
          Fluid_P_I__Form_Test.o Fluid_P_I__Form.o \
          Fluid_P__Template.o Fluid_D__Form.o FluidFeatures_Template.o \
          Mathematics.a Basics.a $(LIBRARIES)

FluidFeatures_P__Form_Test: Basics.a Mathematics.a FluidFeatures_P__Form_Test.o
	$(LINK) $(FLAGS) \
          -o FluidFeatures_P__Form_Test_$(MACHINE) \
          FluidFeatures_P__Form_Test.o FluidFeatures_P__Form.o \
          Fluid_P_I__Form.o Fluid_P__Template.o Fluid_D__Form.o \
          FluidFeatures_Template.o \
          Mathematics.a Basics.a $(LIBRARIES)

Tally_F_D__Form_Test: Basics.a Mathematics.a Spaces.a Tally_F_D__Form_Test.o
	$(LINK) $(FLAGS) \
          -o Tally_F_D__Form_Test_$(MACHINE) \
          Tally_F_D__Form_Test.o Tally_F_D__Form.o Fluid_D__Form.o \
          FluidFeatures_Template.o \
          Spaces.a Mathematics.a Basics.a $(LIBRARIES)

Tally_F_P__Form_Test: Basics.a Mathematics.a Spaces.a Tally_F_P__Form_Test.o
	$(LINK) $(FLAGS) \
          -o Tally_F_P__Form_Test_$(MACHINE) \
          Tally_F_P__Form_Test.o Tally_F_P__Form.o Tally_F_D__Form.o \
          Fluid_P_I__Form.o Fluid_P__Template.o Fluid_D__Form.o \
          FluidFeatures_Template.o \
          Spaces.a Mathematics.a Basics.a $(LIBRARIES)

Sources_F__Form_Test: Basics.a Mathematics.a Sources_F__Form_Test.o
	$(LINK) $(FLAGS) \
          -o Sources_F__Form_Test_$(MACHINE) \
          Sources_F__Form_Test.o Sources_F__Form.o Fluid_P__Template.o \
          Fluid_D__Form.o FluidFeatures_Template.o \
          Mathematics.a Basics.a $(LIBRARIES)

clean:
	rm -f *.o *.a *.mod *.ld

clobber: clean
	rm -f Fluid_D__Form_Test_*
	rm -f Fluid_P_I__Form_Test_*
	rm -f FluidFeatures_P__Form_Test_*
	rm -f Tally_F_D__Form_Test_*
	rm -f Tally_F_P__Form_Test_*
	rm -f Sources_F__Form_Test_*

include $(PATH_BASICS)/Makefile_Dependencies
include $(PATH_MATHEMATICS)/Makefile_Dependencies
include $(PATH_SPACES)/Makefile_Dependencies
include $(PATH_FLUIDS)/Makefile_Dependencies

