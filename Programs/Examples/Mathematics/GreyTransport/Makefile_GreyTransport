  SineWaveDiffusion.o
	$(LINK) $(FFLAGS) \
          -o SineWaveDiffusion_$(MACHINE) \
          SineWaveDiffusion.o SineWaveDiffusion_Form.o \
          GreyMoments.a Fluid.a Mathematics.a Basics.a \
          $(THIRD_PARTY) $(LIBRARIES)

