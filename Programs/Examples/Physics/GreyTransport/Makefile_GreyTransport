          Thermalization_G_G.o Thermalization_G_G__Form.o \
          Interactions_G_G_ASC__Form.o Interactions_G_G_CSL__Form.o \
          Interactions_G_G__Form.o \
          GenASiS.a ThirdParty.a $(LIBRARIES)

