clean: clean_ThirdParty
	rm -f *.o *.a *.mod *.ld

clobber: clean
	rm -f SineWaveStreaming_G_*
	rm -f SineWaveDiffusion_*
	rm -f HomogeneousSphere_*
	rm -f MarshakWave_*
	rm -f StreamingProfile_*

